@code {
    [Parameter] public EventCallback<List<ClariSLiveSetList.Shared.Models.LiveData>> OnLiveDataChangeEventCallback { get; set; }
    [Parameter] public ClariSLiveSetList.Shared.Models.LiveDataManager? LiveManager { get; set; }

    string SelectedValue = "";

    private void OnClickLiveTitle()
    {
        List<ClariSLiveSetList.Shared.Models.LiveData>? livedata
        = LiveManager.GetLiveData(SelectedValue);
        OnLiveDataChangeEventCallback.InvokeAsync(livedata);
    }

    private void OnClickLiveTitleWithPlace( string title, string place )
    {
        List<ClariSLiveSetList.Shared.Models.LiveData>? livedata
        = LiveManager.GetLiveData(title, place);
        OnLiveDataChangeEventCallback.InvokeAsync(livedata);
    }

    private void OnClickLiveTitleWithPlaceAndTime( string title, string place, DateTime date )
    {
        List<ClariSLiveSetList.Shared.Models.LiveData>? livedata
        = LiveManager.GetLiveData(title, place, date);
        OnLiveDataChangeEventCallback.InvokeAsync(livedata);
    }
}
